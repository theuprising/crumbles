{
  "name": "crumbles",
  "version": "3.3.2",
  "files": [
    "dist"
  ],
  "main": "./dist/node.js",
  "browser": "./dist/browser.js",
  "license": "MIT",
  "scripts": {
    "install:types": "flow-typed install",
    "test": "snackstack test",
    "repl": "snackstack repl",
    "build": "snackstack build",
    "docs": "snackstack docs",
    "prepublish": "npm run test && npm run build && npm run docs"
  },
  "precommit": "test",
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "ignore": [
      "docs",
      "flow-typed",
      "type-definitions"
    ]
  },
  "babel": {
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ],
    "presets": [
      [
        "latest",
        {
          "modules": false
        }
      ],
      "stage-0",
      "react"
    ]
  },
  "dependencies": {
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "ramda": "0.21.x"
  },
  "devDependencies": {
    "babel-loader": "^6.2.7",
    "documentation": "^4.0.0-beta12",
    "eslint-plugin-flowtype": "^2.25.0",
    "flow-bin": "^0.34.0",
    "flow-typed": "^2.0.0",
    "jest": "^16.0.2",
    "pre-commit": "^1.1.3",
    "snackstack": "0.0.11",
    "standard": "^8.5.0",
    "webpack": "2.1.0-beta.25"
  }
}
