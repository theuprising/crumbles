{
  "name": "crumbles",
  "version": "3.0.3",
  "files": [
    "dist"
  ],
  "main": "./dist/node.js",
  "browser": "./dist/browser.js",
  "license": "MIT",
  "scripts": {
    "install:types": "flow-typed install",
    "test": "node ./node_modules/.bin/snackstack test",
    "repl": "node ./node_modules/.bin/snackstack repl",
    "build": "node ./node_modules/.bin/snackstack build"
  },
  "pre-commit": "test",
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "ignore": [
      "docs",
      "flow-typed",
      "type-definitions"
    ]
  },
  "babel": {
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ],
    "presets": [
      "latest",
      "stage-0",
      "react"
    ]
  },
  "dependencies": {
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "ramda": "0.21.x"
  },
  "devDependencies": {
    "babel-loader": "^6.2.7",
    "documentation": "^4.0.0-beta12",
    "flow-bin": "^0.34.0",
    "flow-typed": "^2.0.0",
    "jest": "^16.0.2",
    "pre-commit": "^1.1.3",
    "snackstack": "^0.0.1",
    "standard": "^8.5.0",
    "webpack": "^1.13.3"
  }
}
